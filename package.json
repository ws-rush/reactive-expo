{
  "name": "reactive-expo",
  "version": "1.0.0",
  "main": "src/root.ts",
  "scripts": {
    "start": "expo start -c",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild --clean",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format:check": "prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "dependencies": {
    "@expo-google-fonts/exo": "^0.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@formatjs/intl-locale": "^4.2.10",
    "@formatjs/intl-pluralrules": "^5.4.3",
    "@lingui/core": "^5.3.2",
    "@lingui/metro-transformer": "^5.3.2",
    "@lingui/react": "^5.3.2",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/drawer": "^7.1.2",
    "@react-navigation/native": "^7.0.15",
    "expo": "^53.0.13",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.2",
    "expo-font": "^13.3.1",
    "expo-linking": "~7.1.5",
    "expo-localization": "^16.1.5",
    "expo-router": "~5.1.1",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.9",
    "expo-updates": "^0.28.15",
    "expo-web-browser": "~14.2.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "4.11.1",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@lingui/cli": "^5.3.2",
    "@lingui/macro": "^5.3.2",
    "@types/react": "~19.0.14",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~9.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "react-native-svg-transformer": "^1.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
