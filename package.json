{
  "name": "reactive-expo",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start -c",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "web": "expo start --web",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format:check": "prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "extract": "lingui extract",
    "compile": "lingui compile"
  },
  "dependencies": {
    "@expo-google-fonts/exo": "^0.2.3",
    "@expo/vector-icons": "^14.0.4",
    "@formatjs/intl-locale": "^4.2.10",
    "@formatjs/intl-pluralrules": "^5.4.3",
    "@lingui/core": "^4.14.1",
    "@lingui/react": "^4.14.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/drawer": "^7.1.2",
    "@react-navigation/native": "^7.0.15",
    "expo": "^52.0.37",
    "expo-build-properties": "~0.13.2",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.12",
    "expo-font": "^13.0.4",
    "expo-linking": "~7.0.5",
    "expo-localization": "^16.0.1",
    "expo-router": "~4.0.17",
    "expo-splash-screen": "^0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-updates": "^0.27.2",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.7",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@lingui/cli": "^4.14.1",
    "@lingui/macro": "^4.14.1",
    "@types/react": "~18.3.18",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^9.22.0",
    "eslint-config-universe": "^12.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
}
