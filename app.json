{
  "expo": {
    "name": "reactive-expo",
    "slug": "reactive-expo",
    "version": "1.0.0",
    "scheme": "reactive-expo",
    "newArchEnabled": true,
    "plugins": [
      "expo-localization",
      [
        "expo-router",
        {
          "origin": "https://reactive-expo.netlify.app"
        }
      ],
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "dark": {
        "image": "./src/assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      }
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "reactive.expo"
    },
    "android": {
      "package": "reactive.expo",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./src/assets/favicon.png"
    }
  }
}
